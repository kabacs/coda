comment {
    color:#4f6e64;
    font-style:italic;
  }
  
  constant.numeric {
    color:#98ad0d;
  }
  
  constant.numeric.keyword {
    color:#26a6a6;
  }
  
  keyword {
    color:#26a6a6;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#98ad0d;
  }
  
  language.operator {
    color:#26a6a6;
  }
  
  language.property {
    color:#ff5d38;
  }
  
  language.variable {
    color:#2a332b;
  }
  
  markup.comment {
    color:#4f6e64;
  }
  
  markup.constant.entity {
    color:#ff5d38;
  }
  
  markup.declaration {
    color:#4f6e64;
  }
  
  markup.inline.cdata {
    color:#98ad0d;
  }
  
  markup.processing {
    color:#4f6e64;
    font-style:italic;
  }
  
  markup.tag {
    color:#26a6a6;
    font-weight:bold;
  }
  
  markup.tag.attribute.name {
    color:#ff5d38;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#98ad0d;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#d8ebe5;
    color:#2a332b;
  }
  
  meta.highlight.currentline {
    background-color:#c5d8d2;
  }
  
  meta.important {
    color:#26a6a6;
  }
  
  meta.invalid {
    color:#e61f44;
    font-weight:bold;
  }
  
  meta.invisible.characters {
    color:#9aafa9;
  }
  
  meta.link {
    color:#98ad0d;
    font-underline:none;
  }
  
  string {
    color:#98ad0d;
  }
  
  string.regex {
    color:#ff5d38;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
    font-weight:bold;
  }
  
  style.comment {
    color:#4f6e64;
    font-style:italic;
    font-weight:normal;
  }
  
  style.property.name {
    color:#ff5d38;
  }
  
  style.value.color.rgb-value {
    color:#98ad0d;
  }
  
  style.value.keyword {
    color:#26a6a6;
  }
  
  style.value.numeric {
    color:#98ad0d;
  }
  
  style.value.string {
    color:#98ad0d;
  }
  
  support {
    color:#ff5d38;
  }
  