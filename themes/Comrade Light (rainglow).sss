comment {
    color:#9ba5a5;
  }
  
  constant.numeric {
    color:#4eadad;
  }
  
  constant.numeric.keyword {
    color:#7f9689;
  }
  
  keyword {
    color:#7f9689;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#343939;
  }
  
  language.operator {
    color:#7f9689;
  }
  
  language.property {
    color:#7f9689;
  }
  
  language.variable {
    color:#343939;
  }
  
  markup.comment {
    color:#9ba5a5;
  }
  
  markup.constant.entity {
    color:#7f9689;
  }
  
  markup.declaration {
    color:#9ba5a5;
  }
  
  markup.inline.cdata {
    color:#63a5a5;
  }
  
  markup.processing {
    color:#9ba5a5;
  }
  
  markup.tag {
    color:#c24e4b;
  }
  
  markup.tag.attribute.name {
    color:#7f9689;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#63a5a5;
    font-weight:normal;
  }
  
  meta.default {
