comment {
    color:#515e66;
  }
  
  constant.numeric {
    color:#2e6fd9;
  }
  
  constant.numeric.keyword {
    color:#bbbcc4;
  }
  
  keyword {
    color:#bbbcc4;
    font-weight:normal;
  }
  
  keyword.control {
