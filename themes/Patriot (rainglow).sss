  language.variable {
    color:#cad9e3;
  }
  
  markup.comment {
    color:#515e66;
  }
  
  markup.constant.entity {
    color:#2e6fd9;
  }
  
  markup.declaration {
    color:#515e66;
  }
  
  markup.inline.cdata {
    color:#3790de;
  }
  
  markup.processing {
    color:#515e66;
  }
  
  markup.tag {
    color:#de333c;
