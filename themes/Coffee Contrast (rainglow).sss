    color:#a58163;
  }
  
  string.regex {
    color:#f5f3eb;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
  }
  
  style.comment {
    color:#423738;
    font-weight:normal;
  }
  
  style.property.name {
    color:#f5f3eb;
  }
  
  style.value.color.rgb-value {
    color:#cc8f5d;
