comment {
    color:#423738;
  }
  
  constant.numeric {
    color:#cc8f5d;
  }
  
  constant.numeric.keyword {
    color:#80b2b0;
  }
  
  keyword {
    color:#80b2b0;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#e77757;
  }
  
  language.operator {
    color:#80b2b0;
  }
  
  language.property {
    color:#f5f3eb;
  }
  
  language.variable {
    color:#c4babb;
  }
  
  markup.comment {
    color:#423738;
  }
  
  markup.constant.entity {
    color:#f5f3eb;
  }
  
  markup.declaration {
    color:#423738;
  }
  
  markup.inline.cdata {
    color:#a58163;
  }
  
  markup.processing {
    color:#423738;
  }
  
  markup.tag {
    color:#0a9f9b;
  }
  
  markup.tag.attribute.name {
    color:#f5f3eb;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#a58163;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#0f0c0c;
    color:#c4babb;
  }
  
  meta.highlight.currentline {
