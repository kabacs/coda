comment {
    color:#36345a;
  }
  
  constant.numeric {
    color:#e524a9;
  }
  
  constant.numeric.keyword {
    color:#e5194b;
  }
  
  keyword {
    color:#e5194b;
    font-weight:normal;
  }
  
  keyword.control {
