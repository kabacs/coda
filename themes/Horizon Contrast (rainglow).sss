  }
  
  language.variable {
    color:#edebe6;
  }
  
  markup.comment {
    color:#36345a;
  }
  
  markup.constant.entity {
    color:#b133dc;
  }
  
  markup.declaration {
    color:#36345a;
  }
  
  markup.inline.cdata {
    color:#fd8a25;
  }
  
  markup.processing {
    color:#36345a;
  }
  
  markup.tag {
    color:#fd8a25;
  }
  
  markup.tag.attribute.name {
    color:#b133dc;
    font-weight:normal;
