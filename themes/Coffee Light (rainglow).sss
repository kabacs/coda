comment {
    color:#a59d9e;
  }
  
  constant.numeric {
    color:#cc8f5d;
  }
  
  constant.numeric.keyword {
    color:#80b2b0;
  }
  
  keyword {
    color:#80b2b0;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#e77757;
  }
  
  language.operator {
    color:#80b2b0;
  }
  
  language.property {
    color:#282122;
  }
  
  language.variable {
    color:#282122;
  }
  
  markup.comment {
    color:#a59d9e;
  }
  
  markup.constant.entity {
    color:#282122;
  }
  
  markup.declaration {
    color:#a59d9e;
  }
  
  markup.inline.cdata {
    color:#a58163;
  }
  
  markup.processing {
    color:#a59d9e;
  }
  
  markup.tag {
    color:#0a9f9b;
  }
  
  markup.tag.attribute.name {
    color:#282122;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#a58163;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#f4f2f2;
    color:#282122;
  }
  
  meta.highlight.currentline {
