comment {
    color:#2d7a7f;
    font-style:italic;
  }
  
  constant.numeric {
    color:#f78d30;
  }
  
  constant.numeric.keyword {
    color:#30985b;
  }
  
  keyword {
    color:#30985b;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#8ec065;
  }
  
  language.operator {
    color:#30985b;
  }
  
  language.property {
    color:#facc54;
  }
  
  language.variable {
    color:#bad3d0;
  }
  
  markup.comment {
    color:#2d7a7f;
  }
  
  markup.constant.entity {
    color:#facc54;
  }
  
  markup.declaration {
    color:#2d7a7f;
  }
  
  markup.inline.cdata {
    color:#f78d30;
  }
  
  markup.processing {
    color:#2d7a7f;
    font-style:italic;
  }
  
  markup.tag {
    color:#f75431;
    font-weight:bold;
  }
  
  markup.tag.attribute.name {
    color:#facc54;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#f78d30;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#184540;
    color:#bad3d0;
  }
  
  meta.highlight.currentline {
    background-color:#1c514b;
  }
  
  meta.important {
    color:#30985b;
  }
  
  meta.invalid {
    color:#e61f44;
    font-weight:bold;
  }
  
  meta.invisible.characters {
    color:#29776f;
  }
  
  meta.link {
    color:#f78d30;
    font-underline:none;
  }
  
  string {
    color:#f78d30;
  }
  
  string.regex {
    color:#facc54;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
    font-weight:bold;
  }
  
  style.comment {
    color:#2d7a7f;
    font-style:italic;
    font-weight:normal;
  }
  
  style.property.name {
    color:#facc54;
  }
  
  style.value.color.rgb-value {
    color:#f78d30;
  }
  
  style.value.keyword {
    color:#30985b;
  }
  
  style.value.numeric {
    color:#f78d30;
  }
  
  style.value.string {
    color:#f78d30;
  }
  
  support {
    color:#facc54;
  }
  