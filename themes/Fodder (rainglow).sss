comment {
    color:#5f8c75;
    font-style:italic;
  }
  
  constant.numeric {
    color:#f7a21b;
  }
  
  constant.numeric.keyword {
    color:#98d046;
  }
  
  keyword {
    color:#98d046;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#fcfcfa;
  }
  
  language.operator {
    color:#98d046;
  }
  
  language.property {
    color:#98d046;
  }
  
  language.variable {
    color:#cce0d6;
  }
  
  markup.comment {
    color:#5f8c75;
  }
  
  markup.constant.entity {
    color:#98d046;
  }
  
  markup.declaration {
    color:#5f8c75;
  }
  
  markup.inline.cdata {
    color:#f7a21b;
  }
  
  markup.processing {
    color:#5f8c75;
    font-style:italic;
  }
  
  markup.tag {
    color:#ad9895;
    font-weight:bold;
  }
  
  markup.tag.attribute.name {
    color:#98d046;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#f7a21b;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#2d4137;
    color:#cce0d6;
  }
  
  meta.highlight.currentline {
    background-color:#3d594b;
  }
  
  meta.important {
    color:#98d046;
  }
  
  meta.invalid {
    color:#e61f44;
    font-weight:bold;
  }
  
  meta.invisible.characters {
    color:#4b6d5c;
  }
  
  meta.link {
    color:#f7a21b;
    font-underline:none;
  }
  
  string {
    color:#f7a21b;
  }
  
  string.regex {
    color:#98d046;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
    font-weight:bold;
  }
  
  style.comment {
    color:#5f8c75;
    font-style:italic;
    font-weight:normal;
  }
  
  style.property.name {
    color:#98d046;
  }
  
  style.value.color.rgb-value {
    color:#f7a21b;
  }
  
  style.value.keyword {
    color:#98d046;
  }
  
  style.value.numeric {
    color:#f7a21b;
  }
  
  style.value.string {
    color:#f7a21b;
  }
  
  support {
    color:#98d046;
  }
  