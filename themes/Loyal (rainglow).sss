comment {
    color:#655a72;
    font-style:italic;
  }
  
  constant.numeric {
    color:#e5c71b;
  }
  
  constant.numeric.keyword {
    color:#9484d6;
  }
  
  keyword {
    color:#9484d6;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#b3adba;
  }
  
  language.operator {
    color:#9484d6;
  }
  
  language.property {
    color:#ee4266;
  }
  
  language.variable {
    color:#bbb1c9;
  }
  
  markup.comment {
    color:#655a72;
  }
  
  markup.constant.entity {
    color:#ee4266;
  }
  
  markup.declaration {
    color:#655a72;
  }
  
  markup.inline.cdata {
    color:#e5c71b;
  }
  
  markup.processing {
    color:#655a72;
    font-style:italic;
  }
  
  markup.tag {
    color:#3cbbb1;
    font-weight:bold;
  }
  
  markup.tag.attribute.name {
    color:#ee4266;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#e5c71b;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#413e4f;
    color:#bbb1c9;
  }
  
  meta.highlight.currentline {
    background-color:#4f4b60;
  }
  
  meta.important {
    color:#9484d6;
  }
  
  meta.invalid {
    color:#e61f44;
    font-weight:bold;
  }
  
  meta.invisible.characters {
    color:#837c9e;
  }
  
  meta.link {
    color:#e5c71b;
    font-underline:none;
  }
  
  string {
    color:#e5c71b;
  }
  
  string.regex {
    color:#ee4266;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
    font-weight:bold;
  }
  
  style.comment {
    color:#655a72;
    font-style:italic;
    font-weight:normal;
  }
  
  style.property.name {
    color:#ee4266;
  }
  
  style.value.color.rgb-value {
    color:#e5c71b;
  }
  
  style.value.keyword {
    color:#9484d6;
  }
  
  style.value.numeric {
    color:#e5c71b;
  }
  
  style.value.string {
    color:#e5c71b;
  }
  
  support {
    color:#ee4266;
  }
  