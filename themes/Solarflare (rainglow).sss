  markup.declaration {
    color:#777777;
  }
  
  markup.inline.cdata {
    color:#ede574;
  }
  
  markup.processing {
    color:#777777;
  }
  
  markup.tag {
    color:#fc913a;
  }
  
  markup.tag.attribute.name {
    color:#ff4e50;
    font-weight:normal;
