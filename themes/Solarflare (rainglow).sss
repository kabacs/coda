comment {
    color:#777777;
  }
  
  constant.numeric {
    color:#ff4e50;
  }
  
  constant.numeric.keyword {
    color:#ff4e50;
  }
  
  keyword {
    color:#ff4e50;
    font-weight:normal;
  }
  
  keyword.control {
