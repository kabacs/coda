comment {
    color:#324357;
  }
  
  constant.numeric {
    color:#6ee2ff;
  }
  
  constant.numeric.keyword {
    color:#748aa6;
  }
  
  keyword {
    color:#748aa6;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#ffffff;
  }
  
  language.operator {
    color:#748aa6;
  }
  
  language.property {
    color:#89bddd;
  }
  
  language.variable {
    color:#222222;
  }
  
  markup.comment {
    color:#324357;
  }
  
  markup.constant.entity {
    color:#89bddd;
  }
  
  markup.declaration {
    color:#324357;
  }
  
  markup.inline.cdata {
    color:#6ee2ff;
  }
  
  markup.processing {
    color:#324357;
  }
  
  markup.tag {
    color:#267fb5;
  }
  
  markup.tag.attribute.name {
    color:#89bddd;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#6ee2ff;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#d7e5ed;
    color:#222222;
  }
  
  meta.highlight.currentline {
    background-color:#ccdee8;
  }
  
  meta.important {
    color:#748aa6;
  }
  
  meta.invalid {
    color:#e61f44;
  }
  
  meta.invisible.characters {
    color:#ffffff;
  }
  
  meta.link {
    color:#6ee2ff;
    font-underline:none;
  }
  
  string {
    color:#6ee2ff;
  }
  
  string.regex {
    color:#89bddd;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
  }
  
  style.comment {
    color:#324357;
    font-weight:normal;
  }
  
  style.property.name {
    color:#89bddd;
  }
  
  style.value.color.rgb-value {
    color:#6ee2ff;
  }
  
  style.value.keyword {
    color:#748aa6;
  }
  
  style.value.numeric {
    color:#6ee2ff;
  }
  
  style.value.string {
    color:#6ee2ff;
  }
  
  support {
    color:#89bddd;
  }
  