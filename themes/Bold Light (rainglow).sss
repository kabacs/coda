comment {
    color:#cccccc;
  }
  
  constant.numeric {
    color:#f7a21b;
  }
  
  constant.numeric.keyword {
    color:#f0624b;
  }
  
  keyword {
    color:#f0624b;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#888c81;
  }
  
  language.operator {
    color:#f0624b;
  }
  
  language.property {
    color:#f0624b;
  }
  
  language.variable {
    color:#555555;
  }
  
  markup.comment {
    color:#cccccc;
  }
  
  markup.constant.entity {
    color:#f0624b;
  }
  
  markup.declaration {
    color:#cccccc;
  }
  
  markup.inline.cdata {
    color:#f7a21b;
  }
  
  markup.processing {
    color:#cccccc;
  }
  
  markup.tag {
    color:#3d8e91;
  }
  
  markup.tag.attribute.name {
    color:#f0624b;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#f7a21b;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#ffffff;
    color:#555555;
  }
  
  meta.highlight.currentline {
    background-color:#f7f7f7;
  }
  
  meta.important {
    color:#f0624b;
  }
  
  meta.invalid {
    color:#e61f44;
  }
  
  meta.invisible.characters {
    color:#ffffff;
  }
  
  meta.link {
    color:#f7a21b;
    font-underline:none;
  }
  
  string {
    color:#f7a21b;
  }
  
  string.regex {
    color:#f0624b;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
  }
  
  style.comment {
    color:#cccccc;
    font-weight:normal;
  }
  
  style.property.name {
    color:#f0624b;
  }
  
  style.value.color.rgb-value {
    color:#f7a21b;
  }
  
  style.value.keyword {
    color:#f0624b;
  }
  
  style.value.numeric {
    color:#f7a21b;
  }
  
  style.value.string {
    color:#f7a21b;
  }
  
  support {
    color:#f0624b;
  }
  