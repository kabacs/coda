    color:#f55239;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
  }
  
  style.comment {
    color:#524343;
    font-weight:normal;
  }
  
  style.property.name {
    color:#f55239;
  }
  
  style.value.color.rgb-value {
