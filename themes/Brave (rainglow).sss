comment {
    color:#64617a;
    font-style:italic;
  }
  
  constant.numeric {
    color:#4eadad;
  }
  
  constant.numeric.keyword {
    color:#abaab7;
  }
  
  keyword {
    color:#abaab7;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#91b9c9;
  }
  
  language.operator {
    color:#abaab7;
  }
  
  language.property {
    color:#908bbc;
  }
  
  language.variable {
    color:#d6dbdb;
  }
  
  markup.comment {
    color:#64617a;
  }
  
  markup.constant.entity {
    color:#908bbc;
  }
  
  markup.declaration {
    color:#64617a;
  }
  
  markup.inline.cdata {
    color:#63a5a5;
  }
  
  markup.processing {
    color:#64617a;
    font-style:italic;
  }
  
  markup.tag {
    color:#bc4331;
    font-weight:bold;
  }
  
  markup.tag.attribute.name {
    color:#908bbc;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#63a5a5;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#333846;
    color:#d6dbdb;
  }
  
  meta.highlight.currentline {
    background-color:#434a5b;
  }
  
  meta.important {
    color:#abaab7;
  }
  
  meta.invalid {
    color:#e61f44;
    font-weight:bold;
  }
  
  meta.invisible.characters {
    color:#555e75;
  }
  
  meta.link {
    color:#63a5a5;
    font-underline:none;
  }
  
  string {
    color:#63a5a5;
  }
  
  string.regex {
    color:#908bbc;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
    font-weight:bold;
  }
  
  style.comment {
    color:#64617a;
    font-style:italic;
    font-weight:normal;
  }
  
  style.property.name {
    color:#908bbc;
  }
  
  style.value.color.rgb-value {
    color:#4eadad;
  }
  
  style.value.keyword {
    color:#abaab7;
  }
  
  style.value.numeric {
    color:#4eadad;
  }
  
  style.value.string {
    color:#63a5a5;
  }
  
  support {
    color:#908bbc;
  }
  