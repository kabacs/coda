comment {
    color:#594b87;
    font-style:italic;
  }
  
  constant.numeric {
    color:#944fc1;
  }
  
  constant.numeric.keyword {
    color:#725ac1;
  }
  
  keyword {
    color:#725ac1;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#cac4ce;
  }
  
  language.operator {
    color:#725ac1;
  }
  
  language.property {
    color:#8d86c9;
  }
  
  language.variable {
    color:#bdb2f7;
  }
  
  markup.comment {
    color:#594b87;
  }
  
  markup.constant.entity {
    color:#8d86c9;
  }
  
  markup.declaration {
    color:#594b87;
  }
  
  markup.inline.cdata {
    color:#b58ad1;
  }
  
  markup.processing {
    color:#594b87;
    font-style:italic;
  }
  
  markup.tag {
    color:#725ac1;
    font-weight:bold;
  }
  
  markup.tag.attribute.name {
    color:#8d86c9;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#b58ad1;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#242038;
    color:#bdb2f7;
  }
  
  meta.highlight.currentline {
    background-color:#302a4c;
  }
  
  meta.important {
    color:#725ac1;
  }
  
  meta.invalid {
    color:#e61f44;
    font-weight:bold;
  }
  
  meta.invisible.characters {
    color:#3d3660;
  }
  
  meta.link {
    color:#b58ad1;
    font-underline:none;
  }
  
  string {
    color:#b58ad1;
  }
  
  string.regex {
    color:#8d86c9;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
    font-weight:bold;
  }
  
  style.comment {
    color:#594b87;
    font-style:italic;
    font-weight:normal;
  }
  
  style.property.name {
    color:#8d86c9;
  }
  
  style.value.color.rgb-value {
    color:#944fc1;
  }
  
  style.value.keyword {
    color:#725ac1;
  }
  
  style.value.numeric {
    color:#944fc1;
  }
  
  style.value.string {
    color:#b58ad1;
  }
  
  support {
    color:#8d86c9;
  }
  