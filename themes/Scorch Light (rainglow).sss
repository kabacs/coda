comment {
    color:#63383c;
    font-style:italic;
  }
  
  constant.numeric {
    color:#cc8f5d;
  }
  
  constant.numeric.keyword {
    color:#c6bb57;
  }
  
  keyword {
    color:#c6bb57;
    font-weight:normal;
  }
  
  keyword.control {
    font-weight:normal;
  }
  
  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#cf9e51;
  }
  
  language.operator {
    color:#c6bb57;
  }
  
  language.property {
    color:#bd4549;
  }
  
  language.variable {
    color:#514242;
  }
  
  markup.comment {
    color:#63383c;
  }
  
  markup.constant.entity {
    color:#bd4549;
  }
  
  markup.declaration {
    color:#63383c;
  }
  
  markup.inline.cdata {
    color:#a58163;
  }
  
  markup.processing {
    color:#63383c;
    font-style:italic;
  }
  
  markup.tag {
    color:#d39452;
    font-weight:bold;
  }
  
  markup.tag.attribute.name {
    color:#bd4549;
    font-weight:normal;
  }
  
  markup.tag.attribute.value {
    color:#a58163;
    font-weight:normal;
  }
  
  meta.default {
    background-color:#f2eded;
    color:#514242;
  }
  
  meta.highlight.currentline {
    background-color:#eae3e3;
  }
  
  meta.important {
    color:#c6bb57;
  }
  
  meta.invalid {
    color:#e61f44;
    font-weight:bold;
  }
  
  meta.invisible.characters {
    color:#bfb5b5;
  }
  
  meta.link {
    color:#a58163;
    font-underline:none;
  }
  
  string {
    color:#a58163;
  }
  
  string.regex {
    color:#bd4549;
  }
  
  string.regex.escaped {
    color:#355200;
  }
  
  style.at-rule {
    color:#de7a31;
    font-style:normal;
    font-weight:bold;
  }
  
  style.comment {
    color:#63383c;
    font-style:italic;
    font-weight:normal;
  }
  
  style.property.name {
    color:#bd4549;
  }
  
  style.value.color.rgb-value {
    color:#cc8f5d;
  }
  
  style.value.keyword {
    color:#c6bb57;
  }
  
  style.value.numeric {
    color:#cc8f5d;
  }
  
  style.value.string {
    color:#a58163;
  }
  
  support {
    color:#bd4549;
  }
  