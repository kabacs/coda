  keyword.type {
    font-weight:normal;
  }
  
  language.function {
    color:#ebb626;
  }
  
  language.operator {
    color:#39b81f;
  }
  
  language.property {
    color:#d63131;
  }
  
  language.variable {
    color:#ede0ce;
  }
  
  markup.comment {
    color:#7a7267;
  }
  
  markup.constant.entity {
    color:#d63131;
  }
  
  markup.declaration {
    color:#7a7267;
  }
  
  markup.inline.cdata {
    color:#ebb626;
  }
  
  markup.processing {
    color:#7a7267;
  }
  
  markup.tag {
    color:#39b81f;
  }
  
  markup.tag.attribute.name {
    color:#d63131;
    font-weight:normal;
